{
  "$schema": "http://json.schemastore.org/prettierrc",
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "endOfLine": "lf",
  "proseWrap": "always",
  "overrides": [
    {
      "files": [
        "**/jsr.json"
      ],
      "options": {
        "parser": "json-stringify"
      }
    }
  ]
}
